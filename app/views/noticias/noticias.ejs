<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Noticias</title>
		</head>
	<body>

		<h1>Noticias</h1>

		<table>
			<thead>
				<tr>
					<td>ID</td>
					<td>Titulo</td>
					<td>Noticia</td>
				</tr>
			</thead>
			<tbody>
				<% for (let noticia of noticias) { %>
					<tr>
						<td><%= noticia.id_noticia%></td>
						<td><%= noticia.titulo%></td>
						<td><%= noticia.noticia%></td>
					</tr>
				<% } %>
			</tbody>
		</table>
	</body>
</html>